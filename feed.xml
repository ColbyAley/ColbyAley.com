---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Colby Aley</title>
    <link>http://colbyaley.com/</link>
    <atom:link href="http://colbyaley.com/feed.xml" rel="self" type="application/rss+xml" />
    <description>Colby Aley</description>
    <language>en-us</language>
    <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://colbyaley.com{{ post.url }}.html</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Colby Aley</author>
      <guid>http://colbyaley.com{{ post.id }}.html</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>